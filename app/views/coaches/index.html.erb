<%# <h1>Coaches#index</h1>
<p>Find me in app/views/coaches/index.html.erb</p> %>
<h2>All Coaches</p>
<% if @coaches.empty? %>
  <p>There are no coaches in the NBA.</p>
<%else%>
  <ul>
    <% @coaches.each do |coach| %>
      <li>
        <%= link_to coach.name, coach_path(coach) %> is the coach of
        <%= link_to coach.team.name, coach.team %>
      </li>
      <% end %>
  </ul>
<% end %>