<%# <h1>Teams#index</h1>
<p>Find me in app/views/teams/index.html.erb</p> %>
<%if @teams.empty?%>
  <p>There are no teams available.</p>
<%else%>
<% @teams.each do |team| %>
  <h2><%= team.name %></h2>
  <p><%= team.name %> have <%= team.players.count %> players.</p>
  <p><%= team.name %> have won <%= team.points %> games this season.</p>
  <p>To learn more about <%= link_to team.name, team %>.</p>
  <%end%>
<%end%>